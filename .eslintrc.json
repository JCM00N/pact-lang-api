{
 "parserOptions": { "ecmaVersion": 8, "sourceType": "module" },
   "env": {
    "node": true,
    "es6": true
  },

  "rules": {
    "quotes": ["error", "single"],
    "operator-linebreak": ["error", "after", { "overrides": { "?": "before", ":": "before" }}],
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "ignore"
    }]
  }
}
